{
	"name": "pipeline_copysqlonprmtoadls",
	"properties": {
		"activities": [
			{
				"name": "Copy data onprems",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 3,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
