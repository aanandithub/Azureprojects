					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": "select * from testtable",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
