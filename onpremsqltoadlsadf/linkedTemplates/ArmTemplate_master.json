{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "onpremsqltoadlsadf"
		},
		"AzureDataLakeStorage1onpremLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1onpremLS'"
		},
		"AzureDataLakeStorage1onpremLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://onpremstrgacc.dfs.core.windows.net/"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://onpremsqlserverkeyvault1.vault.azure.net/"
		},
		"SqlServer1onpremls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "127.0.0.1"
		},
		"SqlServer1onpremls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "TestDB"
		},
		"SqlServer1onpremls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adf_user"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "onpremsqltoadlsadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1onpremLS_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1onpremLS_accountKey')]"
					},
					"AzureDataLakeStorage1onpremLS_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1onpremLS_properties_typeProperties_url')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"SqlServer1onpremls_properties_typeProperties_server": {
						"value": "[parameters('SqlServer1onpremls_properties_typeProperties_server')]"
					},
					"SqlServer1onpremls_properties_typeProperties_database": {
						"value": "[parameters('SqlServer1onpremls_properties_typeProperties_database')]"
					},
					"SqlServer1onpremls_properties_typeProperties_userName": {
						"value": "[parameters('SqlServer1onpremls_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}